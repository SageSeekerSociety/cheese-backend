{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testMatch": [
    "**/*.e2e-spec.ts",
    "**/*.spec.ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest",
    "\\.m?jsx?$": "jest-esm-transformer"
  },
  "collectCoverageFrom": [
    "src/*/**/*.ts",
    "!src/*/**/*.dto.ts",
    "!src/*/**/*.es-doc.ts",
    "!src/*/**/*.enum.ts",
    "!src/common/config/configuration.ts"
  ],
  "testTimeout": 60000
}