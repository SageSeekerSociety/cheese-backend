{
  "name": "cheese-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build-prisma": "prisma-import -s \"src/**/*.prisma\" -o prisma/schema.prisma --force && prisma generate",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --forceExit",
    "test:watch": "jest --watch --forceExit",
    "test:cov": "jest --coverage --forceExit",
    "test:debug": "node --nolazy --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest.js --runInBand --forceExit",
    "format": "prettier --write {src,test}/**/*.ts && git status",
    "lint": "eslint {src,test}/**/*.ts",
    "lint:fix": "eslint --fix {src,test}/**/*.ts",
    "prepare": "node .husky/install.mjs"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/typeorm": "^10.0.1",
    "@prisma/client": "5.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "ajv": "^8.12.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "nestjs-request-context": "^3.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "prisma": "^5.9.1",
    "prisma-import": "^1.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-json-schema-generator": "^1.5.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
