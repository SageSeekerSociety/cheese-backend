enum AvatarType {
  default
  predefined
  upload
}


model Avatar {
  id         Int      @id @default(autoincrement())
  url        String   @db.VarChar
  name       String   @db.VarChar
  createdAt  DateTime @default(now()) @db.Timestamptz(6)
  avatarType AvatarType
  usageCount Int      @default(0)
}
